function wrong-ok-patches()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3
    local KIND=$4
    local REF=$5

    [[ -f toto ]] && echo this-patch.diff
}

function wrong-nok-patches()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3
    local REF=$3

    [[ $KIND == "true" ]] && echo this.diff
}

