
function hookskind-common-config-options()
{
    echo opt-common
}

function hookskind-default-config-options()
{
    echo opt-default
}

function hookskind-var-config-options()
{
    echo opt-var
}

function hookskind-config-options()
{
    echo opt-std
}

function hookskind-default-config()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3

    echo CONF-DEF $(get-config-options $PREFIX $TARGET $VARIANT)
}

function hookskind-config()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3

    echo CONF-STD $(get-config-options $PREFIX $TARGET $VARIANT)
}

function hookskind-var-config()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3

    echo CONF-VAR $(get-config-options $PREFIX $TARGET $VARIANT)
}

function hookskind-build()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3

    echo BUILD-STD
}

function hookskind-var-build()
{
    local PREFIX=$1
    local TARGET=$2
    local VARIANT=$3

    echo BUILD-VAR
}
