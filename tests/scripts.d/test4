
function test4-vcs()
{
   echo mercurial
   echo ${TESTREPOS}/simple.mercurial.bundle
}

function test4-out-of-tree()
{
    echo false
}

function test4-config()
{
   local PREFIX=$1
   local TARGET=$2

   ./configure
}

function test4-build()
{
   local PREFIX=$1
   local TARGET=$2

   make
}

function test4-tests()
{
   local PREFIX=$1
   local TARGET=$2

   ./main
   return 0
}
