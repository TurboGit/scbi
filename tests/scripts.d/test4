
function test4-vcs()
{
   echo git
   echo ${TESTREPOS}/lib1.bundle
}

function test4-tar()
{
    VERSION=$1

    echo none
    echo none
    echo lib1-$VERSION-src.tar.bz2
}

function test4-build()
{
   PREFIX=$1
   TARGET=$2

   rm *.o
   make
}

function test4-install()
{
   PREFIX=$1
   TARGET=$2

   mkdir -p $PREFIX/include
   mkdir -p $PREFIX/lib
   cp liblib1.so $PREFIX/lib
   cp lib1.h $PREFIX/include
}

function test4-tests()
{
   PREFIX=$1
   TARGET=$2

   ./main
}
