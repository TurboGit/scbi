
function mainmult-vcs()
{
   echo git
   echo ${TESTREPOS}/mainmult.bundle
}

function mainmult-depends()
{
   echo lib1
   echo lib2
}

function mainmult-build()
{
   PREFIX=$1
   TARGET=$2

   rm *.o
   make
}

function mainmult-install()
{
   PREFIX=$1
   TARGET=$2

   mkdir -p $PREFIX/bin
   cp mainmult $PREFIX/bin
}

function mainmult-tests()
{
   PREFIX=$1
   TARGET=$2

   ./mainmult
}
