
function test2-vcs()
{
   echo git
   echo ${TESTREPOS}/test1.bundle
}

function test2-config()
{
   PREFIX=$1
   TARGET=$2

   ./configure
}

function test2-build()
{
   PREFIX=$1
   TARGET=$2

   make
}

function test2-install()
{
   PREFIX=$1
   TARGET=$2

   mkdir -p $PREFIX/bin $PREFIX/include
   cp main $PREFIX/bin
   cp incl.h $PREFIX/include
}
