
function supermod-setup()
{
    local PREFIX=$1
    local TARGET=$2

    mkdir -p $PREFIX
    echo do setup >> $PREFIX/supermod
}

function supermod-best-plan()
{
    echo var
}

function supermod-modules()
{
    echo lib1
    echo lib2
}

function supermod-wrapup()
{
    local PREFIX=$1
    local TARGET=$2

    echo do wrapup >> $PREFIX/supermod
}
