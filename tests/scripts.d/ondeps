
function ondeps-vcs()
{
    echo default
    echo none
    echo git
    echo ${TESTREPOS}/lib1.bundle
}

function ondeps-default-build-depends()
{
    echo liblua5.2-dev
}

function ondeps-depends()
{
    echo os@-mylualib
}

function ondeps-2-depends()
{
    echo os@-mynotfound
}

function ondeps-config()
{
    echo CONFIG
}

function ondeps-build()
{
    echo CONFIG
}

function ondeps-prefix()
{
    echo none
}
