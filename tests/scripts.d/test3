
function test3-vcs()
{
   echo git
   echo ${TESTREPOS}/test1.bundle
}

function test3-config()
{
   local PREFIX=$1
   local TARGET=$2

   ./configure
}

function test3-build()
{
   local PREFIX=$1
   local TARGET=$2

   make
}

function test3-install()
{
   local PREFIX=$1
   local TARGET=$2

   mkdir -p $PREFIX/bin
   cp main $PREFIX/bin
}
