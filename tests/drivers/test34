# test build plan with a :dev version

# =================== module version (v1)
RUNS ./scbi --plan=dev --module-version lib1
# add eof to osht output
echo >> ${OSHT_STDIO}

DIFF <<EOF
v1
EOF

# =================== module version (v2)
RUNS ./scbi --plan=dev --module-version lib1:v2
echo >> ${OSHT_STDIO}

DIFF <<EOF
v2
EOF

# =================== module version (NONE)
RUNS ./scbi --module-version lib1
echo >> ${OSHT_STDIO}

DIFF <<EOF
NONE
EOF
