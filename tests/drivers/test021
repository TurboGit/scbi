# echo ========== dmain
RUNS ./scbi --list-depends dmain

CLEAN-DIFF <<EOF
depA
depB
dmain
dmod
oot
EOF

# echo ========== dmain externals
RUNS ./scbi --list-externals:only dmain

CLEAN-DIFF <<EOF
libaws-dev
EOF

# echo ========== dmain modules & externals
RUNS ./scbi --list-externals dmain

CLEAN-DIFF <<EOF
depA
depB
dmain
dmod
libaws-dev
oot
EOF

# echo ========== dmain direct modules & externals
RUNS ./scbi --list-depends:direct --list-externals dmain

CLEAN-DIFF <<EOF
depA
depB
dmod
libaws-dev
EOF
