# change depA, check that depA is rebuilt and also smain
# which is depending on depA via a meta-module.

cp scripts.d/depA scripts.d/depA.keep

echo >> scripts.d/depA

RUNS ./scbi --deps smain

CLEAN-DIFF <<EOF
