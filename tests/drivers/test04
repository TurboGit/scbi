# check that different versions are properly built
#
# use local environment

REP=$PWD/.root/builds/lib1

./scbi --tests lib1
./scbi --tests --tar lib1:v1
./scbi --tests --tar lib1:v2

if [ -h $REP/tar ]; then
    echo NOK tar present
else
    echo OK tar not present
fi

./scbi --tests --tar lib1:#v1.2

if [ -h $REP/tar ]; then
    echo OK tar present
else
    echo NOK tar not present
fi

./scbi --tests lib1
