# Note that we transform all 1 to A and 2 to B to have a better check
# of the final output.

function o()
{
    cat > out
    # sort lines exept first and last
    head -1 out
    sed '1d' out | sort
    rm out
}

echo "========================= A"
./scbi --plan=group4 --list-plan lib1 | o | tr '[0-9]' 'x'

echo "========================= B"
./scbi --plan=group4/mpi --list-plan lib1 | o | tr '[0-9]' 'x'

echo "========================= C"
./scbi --plan=group4/mpi.fast --list-plan lib1 | o | tr '[0-9]' 'x'
