#  Test inherit no variant

RUNS ./scbi inv-test19

CLEAN-DIFF <<EOF
inv-test19-build is a function
inv-test19-build ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ BUILD " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-config is a function
inv-test19-config ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ CONFIG " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-install is a function
inv-test19-install ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ INSTALL " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-out-of-tree is a function
inv-test19-out-of-tree ()
{
    echo false
}
inv-test19-post-build is a function
inv-test19-post-build ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ POST-BUILD " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-post-config is a function
inv-test19-post-config ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ POST-CONFIG " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-pre-build is a function
inv-test19-pre-build ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ PRE-BUILD " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-pre-install is a function
inv-test19-pre-install ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ PRE-INSTALL " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-pre-setup is a function
inv-test19-pre-setup ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    mkdir \$TARGET-default/src;
    echo "@@@ PRE-SETUP " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-pre-wrapup is a function
inv-test19-pre-wrapup ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ PRE-WRAPUP " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
inv-test19-wrapup is a function
inv-test19-wrapup ()
{
    local PREFIX=\$1;
    local TARGET=\$2;
    echo "@@@ WRAPUP " \$(realpath --relative-to=\$SCBI_BDIR \$PWD)
}
