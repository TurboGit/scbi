
#  Check auto variant with a for-module

RUNS ./scbi --plan=autov --deps mautov

sort-osht-output-n-first-lines 6

CLEAN-DIFF <<EOF
xxxx/xx/xx xx:xx:xx : mainav2=mautov using standard variant
xxxx/xx/xx xx:xx:xx : mainav=mautov using standard variant
xxxx/xx/xx xx:xx:xx : module gcc version 5 matching
xxxx/xx/xx xx:xx:xx : module gcc version 99.99 not found
xxxx/xx/xx xx:xx:xx : module gcc version 99.99 not found
xxxx/xx/xx xx:xx:xx : module mautov using native variant

xxxx/xx/xx xx:xx:xx : Building mautov [native] (5)
xxxx/xx/xx xx:xx:xx : native x86_64-linux-gnu
xxxx/xx/xx xx:xx:xx : steps : setup config build install wrapup
xxxx/xx/xx xx:xx:xx : native-pre-config starting
xxxx/xx/xx xx:xx:xx : native-pre-config completed
xxxx/xx/xx xx:xx:xx : native-config starting
xxxx/xx/xx xx:xx:xx : native-config completed
xxxx/xx/xx xx:xx:xx : native-post-config starting
xxxx/xx/xx xx:xx:xx : native-post-config completed
xxxx/xx/xx xx:xx:xx : native-pre-build starting
xxxx/xx/xx xx:xx:xx : native-pre-build completed
xxxx/xx/xx xx:xx:xx : native-build starting
xxxx/xx/xx xx:xx:xx : native-build completed
xxxx/xx/xx xx:xx:xx : native-post-build starting
xxxx/xx/xx xx:xx:xx : native-post-build completed
xxxx/xx/xx xx:xx:xx : native-pre-install starting
xxxx/xx/xx xx:xx:xx : native-pre-install completed
xxxx/xx/xx xx:xx:xx : native-install starting
xxxx/xx/xx xx:xx:xx : native-install completed
xxxx/xx/xx xx:xx:xx : native-post-install starting
xxxx/xx/xx xx:xx:xx : native-post-install completed
xxxx/xx/xx xx:xx:xx : skip global install
xxxx/xx/xx xx:xx:xx : End Building mautov [native] (5)
EOF

OK  -d .root/builds/mautov/x86_64-linux-gnu-native
NOK -d .root/builds/mautov/x86_64-linux-gnu-mainav
NOK -d .root/builds/mautov/x86_64-linux-gnu-for2

RUNS ./scbi --plan=autov --deps mainav

sort-osht-output-n-first-lines 6

CLEAN-DIFF <<EOF
