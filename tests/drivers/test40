# Note that we transform all 1 to A and 2 to B to have a better check
# of the final output.

# echo ========================= A
RUNS ./scbi --plan=group --list-plan lib1

sort-osht-output-but-first-line

DIFF <<EOF
There is 10 module(s) in the plan
modA
modB
modC
modD:vA
modE/new:vB
modF/grp:vZ
modG/grp:vZ
modH/grp:vZ
modI/hybrid
modJ/speed
EOF

# echo ========================= B
NRUNS ./scbi --plan=group2 --list-plan lib1

CLEAN-DIFF <<EOF
xxxx/xx/xx xx:xx:xx : build (FAILED) build plan unexpected modB on group start
EOF

# echo ========================= C
RUNS ./scbi --plan=group3 --list-plan lib1

sort-osht-output-but-first-line

DIFF <<EOF
There is 6 module(s) in the plan
modA
modB/new:vZ
modC/fast:vc
modD/fast:vA
modE/one:vV
modH/fast:vB
EOF
