
function check-res()
{
    if [[ $1 == 0 ]]; then
        echo FOUND
    else
        echo NOTFOUD
    fi
}

NRUNS ./scbi --store ONE

RUNS ./scbi --store ONE A

RUNS ./scbi --store ONE

echo >> ${OSHT_STDIO}

DIFF <<EOF
A
EOF

RUNS ./scbi --store SECRET "#this-is-a-long:key:with:some:nonstd%*$chars@"
RUNS ./scbi --store SECRET

echo >> ${OSHT_STDIO}

DIFF <<EOF
#this-is-a-long:key:with:some:nonstd%*$chars@
EOF
